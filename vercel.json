{
  "builds": [
    {
      "src": "nuxt.config.ts",
      "use": "@nuxtjs/vercel-builder"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/routes/api.js"
    }
  ],
  "env": {
    "NUXT_PUBLIC_API_BASE_URL": "@nuxt_public_api_base_url",
    "NUXT_PUBLIC_SOCKET_URL": "@nuxt_public_socket_url"
  }
}